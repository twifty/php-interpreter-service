{
  "algorithm": "OPENSSL",
  "chmod": "0755",
  "compression": "GZ",
  "directories": [
    "src"
  ],
  "files": [
    "LICENSE.md",
    "server.php"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "tests",
        "test",
        "Tests",
        "Test",
        "examples"
      ],
      "in": "vendor"
    }
  ],
  "git-version": "package_version",
  "intercept": true,
  "key": ".travis/phar-private.pem",
  "main": "src/main.php",
  "output": "dist/php-interpreter.phar",
  "stub": true
}
